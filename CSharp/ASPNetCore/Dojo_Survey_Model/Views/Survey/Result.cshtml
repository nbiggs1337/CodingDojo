@using Dojo_Survey_Model.Models;
@model Survey
@{
    ViewData["Title"] = "Dojo Survey - Result";
}

<head>
    <link rel="stylesheet" href="~/css/resultstyle.css">
</head>
<body>
    <div class="top"><br><br><br></div>
    <div class="text-center">
        <div class="container">
            <div class="content">
                <div class="top">
                    <h2>Submitted Info</h2>
                </div>
                <div class="bottom">
                    <h3>Name:                      @Model.Name</h3>
                    <h3>Dojo Location:             @Model.Location</h3>
                    <h3>Favorite Language:         @Model.Language</h3>
                    <h3>Comment:                   @Model.Comments</h3>
                </div>
            </div>
        </div>
    </div>
</body>
</html>